{
    "name": "ecolar",
    "version": "0.0.0",
    "description": "Description for ecolar",
    "private": true,
    "license": "UNLICENSED",
    "cacheDirectories": [
        "node_modules"
    ],
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.2",
        "@fortawesome/free-solid-svg-icons": "^5.2.0",
        "@fortawesome/vue-fontawesome": "^0.1.1",
        "axios": "^0.18.0",
        "bootstrap": "^4.1.3",
        "bootstrap-vue": "^2.0.0-rc.11",
        "material-design-icons-iconfont": "^4.0.3",
        "moment": "2.22.2",
        "swagger-ui": "2.2.10",
        "vue": "2.5.17",
        "vue-class-component": "^6.3.2",
        "vue-cookie": "^1.1.4",
        "vue-i18n": "^8.0.0",
        "vue-property-decorator": "^7.2.0",
        "vue-router": "^3.0.1",
        "vue2-filters": "^0.3.0",
        "vuelidate": "^0.7.4",
        "vuetify": "^1.4.0-beta.0",
        "vuex": "^3.0.1",
        "vuex-class": "^0.3.1"
    },
    "devDependencies": {
        "@types/jest": "^23.1.4",
        "@types/node": "^10.12.2",
        "@types/vuelidate": "^0.7.2",
        "@vue/test-utils": "^1.0.0-beta.25",
        "autoprefixer": "^7.1.2",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "^2.2.2",
        "copy-webpack-plugin": "4.6.0",
        "css-loader": "^1.0.0",
        "eslint": "5.0.0",
        "eslint-plugin-vue": "^4.7.1",
        "file-loader": "^1.1.4",
        "friendly-errors-webpack-plugin": "1.7.0",
        "html-webpack-plugin": "3.2.0",
        "jest": "^23.6.0",
        "jest-junit": "^5.2.0",
        "jest-serializer-vue": "^0.2.0",
        "jest-vue-preprocessor": "^1.4.0",
        "merge-jsons-webpack-plugin": "1.0.18",
        "mini-css-extract-plugin": "^0.4.4",
        "node-notifier": "^5.1.2",
        "numeral": "^2.0.6",
        "optimize-css-assets-webpack-plugin": "5.0.1",
        "portfinder": "^1.0.13",
        "postcss-import": "^11.0.0",
        "postcss-loader": "^3.0.0",
        "postcss-url": "^7.2.1",
        "rimraf": "^2.6.0",
        "ts-jest": "^23.0.0",
        "ts-loader": "^5.3.0",
        "tslib": "^1.9.3",
        "typescript": "^3.1.3",
        "uglifyjs-webpack-plugin": "2.0.1",
        "url-loader": "^0.5.8",
        "vue-jest": "^3.0.0",
        "vue-loader": "^15.4.2",
        "vue-template-compiler": "2.5.17",
        "webpack": "4.25.1",
        "webpack-bundle-analyzer": "^2.9.0",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "3.1.10",
        "webpack-merge": "4.1.4",
        "xml2js": "^0.4.17"
    },
    "engines": {
        "node": ">= 6.0.0",
        "npm": ">= 3.0.0"
    },
    "lint-staged": {
        "src/**/*.{ts,css,scss}": [
            "prettier --write",
            "git add"
        ]
    },
    "scripts": {
        "cleanup": "rimraf target/{aot,www}",
        "clean-www": "rimraf target/www/app/{src,target/}",
        "start": "npm run webpack:dev",
        "serve": "npm run start",
        "build": "npm run webpack:prod",
        "webpack:build": "npm run cleanup && npm run webpack:build:main",
        "webpack:build:main": "npm run webpack -- --config webpack/webpack.dev.js --progress --profile",
        "webpack:prod:main": "npm run webpack -- --config webpack/webpack.prod.js --profile",
        "webpack:prod": "npm run cleanup && npm run webpack:prod:main && npm run clean-www",
        "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --progress --inline",
        "webpack:test": "npm run test",
        "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js",
        "webpack": "node --max_old_space_size=4096 node_modules/webpack/bin/webpack.js",
        "lint": "./node_modules/eslint/bin/eslint.js --ext .js,.vue src",
        "test": "./node_modules/jest/bin/jest.js --coverage"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts",
            "vue"
        ],
        "transform": {
            ".*\\.(vue)$": "vue-jest",
            "^.+\\.tsx?$": "ts-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/src/main/webapp/app/$1"
        },
        "testMatch": [
            "<rootDir>/src/test/javascript/spec/app/**/*.spec.ts"
        ],
        "snapshotSerializers": [
            "<rootDir>/node_modules/jest-serializer-vue"
        ]
    },
    "jestSonar": {
        "reportPath": "build/test-results/jest",
        "reportFile": "TESTS-results.xml"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ]
}
